@import "animate.min.css"
@import '/Users/soru/github/caffecaffe/css/font-awesome/css/font-awesome.min.css'
@import "nib/vendor"
@import 'nib/normalize'
vendor-prefixes = o ms moz webkit

body
	font-family 'Lato',sans-serif
	text-align center
footer
	color white
	background-color black
#videoAirbnb
	position relative
	width 100%
	display none
#imagPrincipal
	position  relative
	width  100%
	height  546px
	background-image url('https://raw.githubusercontent.com/soru13/caffecaffe/gh-pages/img/Screen%20Shot%202015-05-10%20at%2012.35.38%20AM.png')
#header
	position absolute
	text-align center
	width  100%
	top  0px
	#blockLanding
		padding-top  130px
	#mynameTitle
		color  white
		line-height  .9em
		margin  0px
		position  relative
	.myCopys
		color  white
		line-height  .9em
		margin  0px
		padding  0px
		position  relative
		margin-top  22px
	#content
		position  relative
		color white
	#imgDevices
		position  relative
		width  200px//para pantalla chica
	#btnMoree
		position  relative
		border-radius  0px
		background  rgba(255,255,255,0.5)
		border-style  none
		top 79px
		&:hover{background-color:  rgba(4, 4, 4, 0.5);}
/*media Query principal airbnb*/
@media screen and (max-width  480px)
	#imgDevices
		display none
@media screen and (max-width  640px)
	#mynameTitle
		font-size  40px//pantalla chica
	.myCopys
		font-size  14px//para pantalla chica
	#content
		font-size  15px//para pantalla chica
@media screen and (min-width  1024px)
	#mynameTitle
		font-size  70px//pantalla grande
	.myCopys
		font-size  24px//para pantalla grande
	#content
		font-size  25px//para pantalla grande
	#videoAirbnb
		display block
	#imagPrincipal
		display none
/*<<<<<<<<<<<<<<<<<<<<<<<<menu>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
#menu
	position fixed
	width 100%
	height 32px
	text-align center
	top  10px
	background  rgba(255,255,255,0.5)
	z-index  1
	a
		color black
		text-decoration none
		cursor pointer
		&:hover
			color gray
			cursor pointer
	.menu
		display inline-block
		vertical-align top
		margin-top  6px
		float right
		margin-right  3%
		&:hover
			cursor pointer
	#brand
		float  left
		margin-left  20px
	.brand
		width 32px
		height 32px
/*<<<<<<<<<<<<<<<<<<<<<<<<end menu>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<<aboutme>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
#aboutme
	text-align center
	height 365px
	background-color #8c8b87
	color white
	h1
		padding-top 40px
		margin-bottom 10px
		margin-top 0px
	#yo
		top -89px
		float right
		text-align right
	.yo
		height 365px
		width 45%
	.block
		display inline-block
		position relative
	#yophoto
		font-family 'Shadows Into Light', cursive
		font-size 27px
		position absolute
		float right
		right 174px
		margin-top 100px
		width 300px
		p
			font-size 16px
/*<<<<<<<<<<<<<<<<<<<<<<<<end aboutme>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/

/*<<<<<<<<<<<<<<<<<<<<<<<<Trust-list>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.Trust-list
	margin 0
	padding 0
	text-align center
	.Trust-company
		display inline-block
		margin 15px 25px
		filter grayscale(100%)
	.sprite-coppel
		width 148px
		height 64px
	.sprite-ayuntamiento
		width 144px
		height 75px
	.sprite-gobierno
		width 125px
		height 155px
	.sprite-imss
		width 100px
		height 40px
	.sprite-tec
		width 100px
		height 40px
	.sprite-premier
		width 174px
		height 53px
	.sprite-monterrey
		width 243px
		height 112px
/*<<<<<<<<<<<<<<<<<<<<<<<<end Trust-list>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
/*<<<<<<<<<<<<<<<<<<<<<<<Contact>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
.Contact-title
	font-weight  bold
	font-size  52px
.Contact-text
	max-width  835px
	margin  30px auto 0 auto
	font-size  21px
.Contact-medias
	margin  40px 0 0
	padding  0
	display  block
	list-style-type  disc
.Contact-media
	display  inline-block
	text-align  center
	margin-bottom  40px
	vertical-align  top
	margin  0
	width 15%
.Contact-mediaName
	text-align  center
	font-size  26px
	margin  0 0 20px
@media screen and (max-width  1200px)
	.Contact-media
		width 100%
/*<<<<<<<<<<<<<<<<<<<<<<<end Contact>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/
